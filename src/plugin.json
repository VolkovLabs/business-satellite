{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "annotations": true,
  "dependencies": {
    "grafanaDependency": ">=8.5.0",
    "plugins": []
  },
  "id": "volkovlabs-nautilus-datasource",
  "info": {
    "author": {
      "name": "Volkov Labs",
      "url": "https://volkovlabs.io"
    },
    "description": "Nautilus Data Source for Grafana",
    "keywords": ["grafana", "api"],
    "links": [
      {
        "name": "Documentation",
        "url": "https://docs.volkovlabs.io"
      },
      {
        "name": "GitHub",
        "url": "https://github.com/volkovlabs/volkovlabs-nautilus-datasource"
      }
    ],
    "logos": {
      "large": "img/logo.svg",
      "small": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Data source",
        "path": "img/datasource.png"
      }
    ],
    "updated": "%TODAY%",
    "version": "%VERSION%"
  },
  "metrics": true,
  "routes": [
    {
      "method": "GET",
      "path": "api",
      "reqRole": "Viewer",
      "url": "{{ .JsonData.url }}/api",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.token }}"
        }
      ]
    }
  ],
  "name": "Nautilus",
  "type": "datasource"
}
